{
  "version": 2,
  "projects": {
    "global": {
      "root": "apps/global",
      "sourceRoot": "apps/global/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/global/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "global:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "global:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/global/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/global"],
          "options": {
            "jestConfig": "apps/global/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "global-e2e": {
      "root": "apps/global-e2e",
      "sourceRoot": "apps/global-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/global-e2e/cypress.json",
            "devServerTarget": "global:serve",
            "tsConfig": "apps/global-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "global:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/global-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "css"
      }
    }
  },
  "defaultProject": "global"
}
