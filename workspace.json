{
  "version": 2,
  "projects": {
    "conference": {
      "root": "apps/conference",
      "sourceRoot": "apps/conference/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/conference/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "conference:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "conference:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/conference/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/conference"],
          "options": {
            "jestConfig": "apps/conference/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "conference-e2e": {
      "root": "apps/conference-e2e",
      "sourceRoot": "apps/conference-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/conference-e2e/cypress.json",
            "devServerTarget": "conference:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "conference:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/conference-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["conference"]
    },
    "global": {
      "root": "apps/global",
      "sourceRoot": "apps/global/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/global/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "global:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "global:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/global/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/global"],
          "options": {
            "jestConfig": "apps/global/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "global-e2e": {
      "root": "apps/global-e2e",
      "sourceRoot": "apps/global-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/global-e2e/cypress.json",
            "devServerTarget": "global:serve",
            "tsConfig": "apps/global-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "global:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/global-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["global"]
    }
  }
}
